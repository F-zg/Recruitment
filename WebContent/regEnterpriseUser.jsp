<script type="text/javascript">
 function valuedate(){
	  var userName=document.getElementById("euname");
	   var userPassword=document.getElementById("eupassword");
	   var eid=document.getElementById("eid");
	   if(userName.value==""){
		   alert("用户名不能为空");
		   userName.focus();
		   return false;
	   } 
	   if(userPassword.value==""){
		   alert("密码不能为空");
		   userPassword.focus();
		   return false;
	   }
	   if(eid.value==""){
		   alert("企业编号不能为空");
		   eid.focus();
		   return false;
	   }
	   return true;
 }
